DISPLAY_MODE_NUMBER
}display_mode_e;

struct status_t
{
unsigned tick:1;
unsigned dot:1;
}status;

uint16_t timer1CountedTicks=(65535U);
uint8_t speed;

void lcdDisplay(uint8_t lcdCharacter, uint8_t lcdPosition)
{
uint8_t requiredSegments;
uint8_t segmentsIndex;
uint8_t segmentAddress;

requiredSegments=charactersToSegments[lcdCharacter];
for(segmentsIndex=0;segmentsIndex<8;segmentsIndex++)
{
segmentAddress=segmentsToLCDDataRegisters[lcdPosition][segmentsIndex];
if(requiredSegments&(1<<segmentsIndex))
*((&LCDDATA0)+segmentAddress/8)|=(1<<(segmentAddress%8));
else
*((&LCDDATA0)+segmentAddress/8)&=~(1<<(segmentAddress%8));
}
}

