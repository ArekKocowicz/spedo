void interrupt my_interrupt(void)
{


if(PIR1bits.TMR1GIF)
{
PIR1bits.TMR1GIF=0;
timer1CountedTicks=TMR1L;
timer1CountedTicks|=(TMR1H<<8);
TMR1H=0;
TMR1L=0;
}

if(INTCONbits.TMR0IF)
{
INTCONbits.TMR0IF=0;
status.tick=1;
}


